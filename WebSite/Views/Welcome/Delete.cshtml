@model Neiralink.Models.RandomWelcome
@{
	ViewData["Title"] = "Delete random welcome";
}
<div class="container margin-top-5">
	<h2>@ViewBag.Title</h2>
	<div>
		<dl class="dl-horizontal">
			<dt>EN</dt>
			<dd>
				@Model.EN
			</dd>
			<dt>RU</dt>
			<dd>
				@Model.RU
			</dd>
			<dt>UA</dt>
			<dd>
				@Model.UK
			</dd>
		</dl>

		<div>
			<form asp-controller="Welcome" asp-action="Delete" method="post">
				<div class="form-group">
					<input type="submit" class="btn btn-default" value="Delete" />
				</div>
			</form>
		</div>
	</div>
</div>