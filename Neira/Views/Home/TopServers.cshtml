@model IEnumerable<Discord.WebSocket.SocketGuild>
@{
	ViewData["Title"] = "TopServers";
}

<h1>TopServers</h1>
<table class="table">
	<thead>
		<tr>
			<th></th>
			<th>Имя</th>
			<th>Стражей</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var guild in Model)
		{
			<tr>
				<td><img src="@guild.IconUrl" /></td>
				<td>@guild.Name</td>
				<td>@guild.MemberCount</td>
			</tr>
		}
	</tbody>
</table>
