<!DOCTYPE html>
<html lang="ru">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<meta name="description" content="Нейроматрица, это дискорд бот в мире Destiny 2">
	<meta name="author" content="Cyber_Garret">

	<title>@ViewData["Title"]</title>

	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
	<link rel="stylesheet" href="~/css/site.css" />
</head>
<body id="page-top">

	@RenderBody()

	<!-- Footer -->
	<footer class="footer text-muted">
		<div class="container">
			<div class="align-items-center">
				<div class="btn-group">
					<a href="https://discord.gg/WcuNPM9" target="_blank">
						<span class="footer-links" data-toggle="tooltip" data-placement="top" title="Дискорд сервер Чёрный исход, домик Нейры.">
							<i class="fab fa-discord"></i>
						</span>
					</a>
					<a href="https://vk.com/failsafe_bot" target="_blank">
						<span class="footer-links" data-toggle="tooltip" data-placement="top" title="Сообщество ВКонтакте">
							<i class="fab fa-vk"></i>
						</span>
					</a>
				</div>
				<br />
				<span class="copyright">&copy; 2018-@DateTime.Now.Year - Разработано <a href="https://github.com/Cyber-Garret">Cyber_Garret</a></span>
			</div>
		</div>
	</footer>

	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/lib/jquery-easing/jquery.easing.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>
	<script src="https://kit.fontawesome.com/f47d5ef618.js" crossorigin="anonymous"></script>

	@RenderSection("Scripts", required: false)
</body>
</html>
